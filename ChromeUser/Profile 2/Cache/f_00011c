./Columns/Invoice';\nimport Reason from './Columns/Reason';\nimport Status from './Columns/Status';\n\nimport ModalAccept from './Modals/AcceptedBySupplier';\nimport ModalIssue from './Modals/Issue';\nimport ModalDetail from './Modals/Detail';\nimport ModalBarcode from './Modals/Barcode';\nimport ModalCargoBoxQuantityDeci from './Modals/CargoBoxQuantityDeci';\nimport BarcodeType from '@/enums/BarcodeType';\nimport GAEvents from '@/enums/GAEvents';\n\nexport default {\n  name: 'ClaimList',\n  components: {\n    Tabs,\n    ScDataTable,\n    FilterBar,\n    ListHeader,\n    OrderInfo,\n    ProductInfo,\n    PackageNo,\n    Invoice,\n    Reason,\n    Status,\n    ScConfirmDialog,\n    ModalAccept,\n    ModalIssue,\n    ModalDetail,\n    ModalBarcode,\n    ModalCargoBoxQuantityDeci,\n  },\n  data () {\n    return {\n      tableConfig: {\n        columns: [\n          {\n            label: 'SİPARİŞ BİLGİLERİ',\n          },\n          {\n            label: 'ALICI',\n            field: 'fullName',\n          },\n          {\n            label: 'ADET',\n          },\n          {\n            label: 'BİLGİLER',\n          },\n          {\n            label: 'BİRİM FİYAT',\n          },\n          {\n            label: 'KARGO',\n          },\n          {\n            label: 'FATURA',\n          },\n          {\n            label: 'İADE SEBEBİ',\n          },\n          {\n            label: 'DURUM',\n          },\n        ],\n        body: [],\n        pagination: {},\n      },\n      barcodeInfos: {},\n      selectedOrders: [],\n      currentPosition: 0,\n      dialogBody: `İade ürün tarafınıza ulaşmadıysa bu işlemi gerçekleştirmeyiniz!\n          İade işleminiz aksiyon bekleyen statüsüne geçecektir. Bu işlemi geri alamazsınız.\n          Ürünün size ulaştığını onaylıyor musunuz?`,\n      isConfirmDialogOpen: false,\n      acceptModal: false,\n      issueModal: false,\n      detailModal: false,\n      barcodeModal: false,\n      barcodeType: BarcodeType.A4,\n      trackingMo