') }}</span>\n              </td>\n            </tr>\n          </tbody>\n        </template>\n      </ScDataTable>\n      <ModalUnSupplied\n        :show=\"unSuppliedModal\"\n        @close=\"unSuppliedModal = false\"\n        @update=\"deleteItemPackage\"\n        :item=\"orders.selectedItem\"\n      />\n      <ModalSplitPackage\n        :show=\"splitPackageModal\"\n        @close=\"splitPackageModal = false\"\n        @update=\"showSplitPackageInformationModal\"\n        :item=\"orders.selectedItem\"\n      />\n      <ModalLabel\n        v-if=\"labelModal\"\n        @close=\"closeLabelModal\"\n        :selected-orders=\"selectedOrders\"\n        :current-position=\"currentPosition\"\n        :barcode-type=\"barcodeType\"\n      />\n      <ModalInstantDeliveryPrintLabel\n        v-if=\"instantDeliveryPrintLabelModal\"\n        @close=\"closeInstantDeliveryPrintLabel\"\n        :selected-orders=\"selectedOrders\"\n        :current-position=\"currentPosition\"\n      />\n      <ModalAddress\n        :show=\"addressModal\"\n        @close=\"addressModal = false\"\n        :item=\"orders.selectedItem\"\n      />\n      <ModalDefective\n        :show=\"defectiveModal\"\n        @printLabel=\"printLabel\"\n        @close=\"defectiveModal = false\"\n        :item=\"orders.selectedItem\"\n      />\n      <ModalAlternativeCargoProvider\n        v-if=\"isModalAlternativeCargoProvider\"\n        :show=\"cargoProviderModal\"\n        @close=\"cargoProviderModal = false\"\n        @update=\"updateItemCargoProvider\"\n        :item=\"orders.selectedItem\"\n        @cargoProviderInfo=\"cargoProviderInfo\"\n      />\n      <ModalCargoProvider\n        v-if=\"!isModalAlternativeCargoProvider\"\n        :show=\"cargoProviderModal\"\n        @close=\"cargoProviderModal = false\"\n        @update=\"updateItemCargoProvider\"\n        :item=\"orders.selectedItem\"\n        @cargoProviderInfo=\"cargoProviderInfo\"\n      />\n      <ModalSplitPackageInformation\n        :show=\"splitPack