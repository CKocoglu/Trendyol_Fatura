lama',\n    TAB_VIEW: 'Tab Gösterimi',\n    INVOICE_VIEW: 'Fatura Bilgilerini Göster',\n    CARGO_TRACKING_ACTION: 'Kargo Takip Et',\n    MY_INVOICE_APPLY: 'ApplyButtonClick',\n  },\n};\n","<template>\n  <nav class=\"navbar navbar-expand-lg navbar-light bg-light custom p-0\">\n    <div class=\"navbar-collapse\">\n      <ul class=\"navbar-nav mr-auto mt-2 mt-lg-0\">\n        <template\n          v-for=\"item in items\"\n        >\n          <li\n            v-if=\"!item.hidden\"\n            :key=\"item.route\"\n          >\n            <RouterLink\n              :to=\"{ name: item.route }\"\n              class=\"col nav-link\"\n              @click.native=\"trackEvent(item)\"\n            >\n              {{ item.text }}\n              <p\n                class=\"text-center\"\n                v-if=\"!item.hideCount\"\n              >\n                {{ item.count }} PAKET\n              </p>\n            </RouterLink>\n          </li>\n        </template>\n      </ul>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport SupplierService from '../../services/SupplierService';\nimport GAEvents from '@/enums/GAEvents';\n\nexport default {\n  name: 'Tabs',\n  data () {\n    return {\n      items: [],\n    };\n  },\n  created () {\n    this.setMenuItems();\n  },\n  methods: {\n    setMenuItems () {\n      this.items = [\n        {\n          route: 'ShipmentPackages',\n          text: this.$t('orders.all'),\n          count: this.totalOrderNotificationCount,\n        },\n        {\n          route: 'ShipmentPackagesReady',\n          text: this.$t('orders.ready'),\n          count: this.orderNotifications.readyToShipPackageCount,\n        },\n        {\n          route: 'ShipmentPackagesShipped',\n          text: this.$t('orders.shipped'),\n          count: this.orderNotifications.shippedPackageCount,\n        },\n        {\n          route: 'ShipmentPackagesDelivered',\n          text: this.$t('orders.delivered'),\n          count: this.orderNotifications.deliveredPackageCount,\n        },\n        {\n          route: 'ShipmentPackagesDefective',\n          text: this.$t('orders.defective'),\n          hideCount: true,\n          hidden: this.isInstantDeliveryPackages,\n        },\n        {\n          route: 'OrdersAwaiting',\n          text: this.$t('orders.awaitingMenu'),\n          hideCount: true,\n          hidden: !this.showOrdersAwaitingMenu,\n        },\n      ];\n    },\n    trackEvent (item) {\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: window.user.storeBaseEnabled ? GAEvents.CATEGORY.SHIPMENT_PACKAGE_INSTANT : GAEvents.CATEGORY.SHIPMENT_PACKAGE_SC,\n        eventAction: GAEvents.ACTION.TAB_VIEW,\n        eventLabel: item.text,\n      });\n    },\n  },\n  computed: {\n    ...mapGetters(['orderNotifications']),\n    totalOrderNotificationCount () {\n      const packageCount = this.orderNotifications;\n      return (\n        packageCount.readyToShipPackageCount +\n        packageCount.shippedPackageCount +\n        packageCount.deliveredPackageCount\n      );\n    },\n    showOrdersAwaitingMenu () {\n      return SupplierService.isTaggedBy(501);\n    },\n    isInstantDeliveryPackages () {\n      return window.user.storeBaseEnabled;\n    },\n  },\n  watch: {\n    orderNotifications: {\n      deep: true,\n      handler () {\n        this.setMenuItems();\n      },\n    },\n    totalOrderNotificationCount () {\n      this.setMenuItems();\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.custom {\n  width: 100%;\n  background-color: #f1f2f7 !important;\n  font-weight: 500;\n  font-size: 13px;\n\n  .hidden {\n    display: none;\n  }\n\n  ul {\n    li {\n      a {\n        height: 65px;\n      }\n    }\n    li:first-child {\n      a {\n        border-top-left-radius: 4px;\n      }\n    }\n    li:last-child {\n      a {\n        border-top-right-radius: 4px;\n      }\n    }\n    .nav-link {\n      display: flex;\n      align-items: center;\