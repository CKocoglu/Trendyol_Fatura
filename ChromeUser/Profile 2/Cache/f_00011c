window.scrollTo(0, 0);\n      });\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: GAEvents.CATEGORY.CLAIM_SC,\n        eventAction: `${this.$route.name} - ${GAEvents.ACTION.PAGINATION}`,\n        eventLabel: p + 1,\n      });\n    },\n    formatPrice (value) {\n      return format.price(value);\n    },\n    accept () {\n      this.acceptModal = true;\n    },\n    issue () {\n      this.issueModal = true;\n    },\n    details () {\n      this.detailModal = true;\n    },\n    takeAction () {\n      this.isConfirmDialogOpen = true;\n    },\n    async acceptTakeAction () {\n      await this.setClaimStatusToWaitingInAction();\n      this.$toast.success('Talebiniz başarıyla alınmıştır.', {\n        duration: 5000,\n      });\n      window.scEventBus.publisher('CHANGE_CLAIM_ITEM_COUNT', {\n        claimStatusToIncrement: 'waitingInActionCount',\n        claimStatusToDecrement: 'createdCount',\n      });\n      this.isConfirmDialogOpen = false;\n    },\n    async barcode (type) {\n      const shipmentPackageFilterDto = this.claims.selectedItem.shipmentPackageFilterDto;\n      const claim = this.claims;\n\n      this.selectedOrders = [];\n\n      if (shipmentPackageFilterDto) {\n        if (shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.UPS_MP.id) {\n          if (shipmentPackageFilterDto.boxQuantity === null &&\n        (shipmentPackageFilterDto.whoPays === null || shipmentPackageFilterDto.whoPays === 0)) {\n            this.$toast.warning('Lütfen sipariş gönderimine ait koli adet bilgilerini giriniz.', {\n              duration: 5000,\n            });\n\n            return;\n          } else if (shipmentPackageFilterDto.boxQuantity !== null &&\n          (shipmentPackageFilterDto.whoPays === null || shipmentPackageFilterDto.whoPays === 0)) {\n            try {\n              const options = {\n                cargoTrackingNumber: shipmentPackageFilterDto.cargoTrackingNumber,\n              };\n              this.barcodeInfos = await orders.getBarcodeInfo(options);\n\n              for (let barcode of this.barcodeInfos.barcodeList) {\n                const selected = _.cloneDeep(claim);\n                selected.selectedItem.shipmentPackageFilterDto.cargoTrackingNumber = barcode;\n                this.selectedOrders.push(selected);\n              }\n\n              this.barcodeType = type;\n              this.barcodeModal = true;\n\n              return;\n            } catch (e) {\n              this.$toast.warning('Paket bilgileriniz UPS tarafından aktarılıyor, lütfen bekleyiniz.', {\n                duration: 5000,\n              });\n              return;\n            }\n          }\n        } else if ((shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.CEVA_MP.id ||\n          shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.HOROZ_MP.id ||\n          shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.BOR_MP.id)) {\n          if ((shipmentPackageFilterDto.boxQuantity === null || shipmentPackageFilterDto.deci === null) &&\n        (shipmentPackageFilterDto.whoPays === null || shipmentPackageFilterDto.whoPays === 0)) {\n            this.$toast.warning('Lütfen sipariş gönderimine ait desi ve koli adet bilgilerini giriniz.', {\n              duration: 5000,\n            });\n\n            return;\n          }\n        }\n        this.selectedOrders.push(claim);\n        this.barcodeType = type;\n        this.barcodeModal = true;\n      }\n\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: GAEvents.CATEGORY.CLAIM_SC,\n        eventAction: type === BarcodeType.ZPL ? GAEvents.ACTION.LABEL_PRINT_ZPL : GAEvents.ACTION.LABEL_PRINT_A4,\n        eventLabel: this.claims.selectedItem.orderNumber,\n      });\n    },\n    async deciBoxQuantity () {\n      const orderNumber = this.claims.selectedItem.orderNumber;\n      const options = {\n        orderNumber,\n      };\n      const order = await orders.getShipmentPackages(options);\n\n      this.updateBoxQuantityAndDeci = false;\n\n      if (order.content) {\n        const item = order.content.find(item => item.orderDetail.orderNumber === orderNumber);\n        const shipmentPackageFilterDto = this.claims.selectedItem.shipmentPackageFilterDto;\n\n        if ((shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.CEVA_MP.id ||\n        shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.HOROZ_MP.id ||\n        shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.BOR_MP.id) &&\n        (item.whoPays === null || item.whoPays === 0)) {\n          this.cargoBoxQuantityDeciModal = true;\n          this.updateBoxQuantityAndDeci = true;\n\n          const options = {\n            boxQuantity: this.cargoBoxQuantity,\n            deci: this.cargoDeci,\n          };\n          await orders.changeDeciBoxQuantity(shipmentPackageFilterDto.id, options);\n\n          if (this.updateBoxQuantityAndDeci) {\n            spinner.start();\n\n            setTimeout(async () => {\n              await this.handleTable();\n\n              spinner.done();\n            }, 2000);\n          }\n        }\n        if (shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.UPS_MP.id &&\n      (item.whoPays === null || item.whoPays === 0)) {\n          this.cargoBoxQuantityDeciModal = true;\n          this.updateBoxQuantityAndDeci = true;\n\n          const options = {\n            boxQuantity: this.cargoBoxQuantity,\n          };\n          await orders.changeDeciBoxQuantity(shipmentPackageFilterDto.id, options);\n\n          if (this.updateBoxQuantityAndDeci) {\n            spinner.start();\n\n            setTimeout(async () => {\n              await this.handleTable();\n\n              spinner.done();\n            }, 2000);\n          }\n        }\n      }\n\n      this.cargoBoxQuantityDeciModal = false;\n    },\n    deciBoxQuantityModal () {\n      this.cargoBoxQuantityDeciModal = true;\n    },\n    cargoParams (options) {\n      this.cargoBoxQuantity = Number(options.boxQuantity);\n      this.cargoDeci = Number(options.deci);\n    },\n  },\n  computed: {\n    ...mapState({\n      claims: state => state.claims,\n    }),\n    isRejectedPage () {\n      const { statuses } = this.$route.meta;\n\n      return statuses === 'Rejected';\n    },\n    showDeci () {\n      const shouldHaveDeciCargoProviders = [CargoProviders.all.CEVA_MP.id, CargoProviders.all.HOROZ_MP.id, CargoProviders.all.BOR_MP.id];\n\n      return shouldHaveDeciCargoProviders\n        .indexOf(this.claims.selectedItem.shipmentPackageFilterDto.cargoProviderId) !== -1;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .list-header {\n    font-size: 16px;\n    font-weight: 500;\n    color: #273142;\n  }\n\n  .claims-row {\n    tr {\n      &:not(:first-child) {\n        td[rowspan] {\n          display: none;\n        }\n      }\n    }\n  }\n</style>\n","import mod from \"-!../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../node_modules/thread-loader/dist/cjs.js!../../../node_modules/babel-loader/lib/index.js!../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./List.vue?vue&type=script&lang=js&\"; export default mod; export * from \"-!../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../node_modules/thread-loader/dist/cjs.js!../../../node_modules/babel-loader/lib/index.js!../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./List.vue?vue&type=script&lang=js&\"","import { render, staticRenderFns } from \"./List.vue?vue&type=template&id=6e768363&scoped=true&\"\nimport script from \"./List.vue?vue&type=script&lang=js&\"\nexport * from \"./List.vue?vue&type=script&lang=js&\"\nimport style0 from \"./List.vue?vue&type=style&index=0&id=6e768363&lang=scss&scoped=true&\"\n\n\n/* normalize component */\nimport normalizer from \"!../../../node_modules/vue-lo