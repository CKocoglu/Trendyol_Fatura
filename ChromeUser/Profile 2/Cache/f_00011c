      { text: 'ÜRÜN BİLGİSİ', key: 'itemInfo' },\n        { text: 'ADET', key: 'quantity', align: 'center', width: '75px' },\n        { text: 'İPTAL ADEDİ', key: 'cancelQuantity', width: '150px' },\n        { text: 'ALTERNATİF ÜRÜN ADEDİ', key: 'alternativeItemQuantity', width: '150px' },\n        { text: 'ÜRÜN TOPLANDI', key: 'collectedItem', width: '180px' },\n      ],\n      items: [],\n      showUnSuppliedConfirmModal: false,\n      showCollectedConfirmModal: false,\n      isUnSuppliedAllItems: false,\n    };\n  },\n  computed: {\n    isCollectedAllItems () {\n      return this.items.every((item) => {\n        return item._isCollected && !item._alternativeItemsCount && !item._unSuppliedItemsCount;\n      });\n    },\n    isSaveButtonEnabled () {\n      return this.items.every(line => line._isCollected || line.packageItemsCount === (line._unSuppliedItemsCount + line._alternativeItemsCount));\n    },\n  },\n  methods: {\n    ...mapActions({\n      updateOrderStatus: 'updateOrderStatus',\n    }),\n    close () {\n      this.showUnSuppliedConfirmModal = false;\n      this.showCollectedConfirmModal = false;\n      this.$emit('close');\n    },\n    errImage (el) {\n      const e = el;\n      e.srcElement.src = this.$images.setImagePublicPath('default.jpg');\n    },\n    setItems () {\n      const lines = this.item.orderLines;\n      const items = Object.keys(lines).map(lineId => ({\n        id: lineId,\n        productName: lines[lineId].productName,\n        productImage: lines[lineId].productImage,\n        brandName: lines[lineId].brandName,\n        barcode: lines[lineId].barcode,\n        packageItemsCount: lines[lineId].packageItemsCount,\n        _isCollected: lines[lineId].isCollected,\n        _alternativeItemsCount: lines[lineId].alternativePackageItemCount,\n        _unSuppliedItemsCount: 0,\n      }));\n      this.items = [...items];\n    },\n    onSelectChange (row) {\n      if (row.packageItemsCount <= (row._unSuppliedItemsCount + row._alternativeItemsCount)) {\n        row._isCollected = false;\n      }\n    },\n    onCollectedChange (row, e) {\n      if (e.checked && row.packageItemsCount <= (row._unSuppliedItemsCount + row._alternativeItemsCount)) {\n        row._unSuppliedItemsCount = 0;\n        row._alternativeItemsCount = 0;\n      }\n    },\n    async onSubmit () {\n      const reqParams = {\n        packageId: String(this.item.id),\n        orderId: String(this.item.orderId),\n        collectedItemIds: [],\n        alternativeItemIds: [],\n        unSuppliedItemIds: [],\n      };\n      this.items.forEach(item => {\n        const packageItems = Object.keys(this.item.orderLines[item.id].packageItems);\n        reqParams.unSuppliedItemIds = [...reqParams.unSuppliedItemIds, ...packageItems.splice(0, item._unSuppliedItemsCount)];\n        reqParams.alternativeItemIds = [...reqParams.alternativeItemIds, ...packageItems.splice(0, item._alternativeItemsCount)];\n        if (item._isCollected) {\n          reqParams.collectedItemIds = [...reqParams.collectedItemIds, ...packageItems];\n        }\n      });\n      try {\n        if (this.isUnSuppliedAllItems) {\n          await this.onSubmitUnSupplied(reqParams.unSuppliedItemIds.map(id => String(id)));\n        } else {\n          await instant.markItems(reqParams, { hideErrorMessage: true });\n        }\n        this.$toast.success(this.$t('commons.successMessage'), {\n          duration: 5000,\n        });\n        this.$emit('submit', this.items);\n        this.close();\n      } catch (e) {\n        this.showUnSuppliedConfirmModal = false;\n        this.showCollectedConfirmModal = false;\n        this.$toast.error(e.response?.data?.error || e.message, {\n          duration: 5000,\n        });\n      }\n    },\n    onCancel () {\n      this.items.forEach(item => {\n        item._isCollected = false;\n        item._alternativeItemsCount = 0;\n        item._unSuppliedItemsCount = item.packageItemsCount;\n      });\n      this.isUnSuppliedAllItems = true;\n      this.showConfirmModal();\n    },\n    showConfirmModal () {\n      if (this.isCollectedAllItems) {\n        this.showCollectedConfirmModal = true;\n      } else {\n        this.showUnSuppliedConfirmModal = true;\n      }\n      this.$nextTick(() => {\n        document.body.classList.add('modal-open');\n      });\n    },\n    async onSubmitUnSupplied (packageItemIds) {\n      await instant.setUnsupplied({\n        orderId: String(this.item.orderId),\n        packageId: String(this.item.id),\n        packageItemIds,\n      }, { hideErrorMessage: true });\n      this.updateOrderStatus('UnSupplied');\n      this.isUnSuppliedAllItems = false;\n    },\n  },\n  mounted () {\n    document.body.classList.add('modal-open');\n    this.setItems();\n  },\n  destroyed () {\n    document.body.classList.remove('modal-open');\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  @import '~@seller-center/grace/css/root.css';\n\n  .edit-modal::v-deep {\n    .modal-dialog {\n      max-width: 1030px;\n    }\n\n    .modal-header {\n      padding: 30px 30px 0;\n      .modal-title {\n        font-size: 20px;\n      }\n    }\n\n    .modal-body {\n      padding: 30px 30px 0;\n      overflow-y: scroll;\n      max-height: calc(100vh - 400px);\n\n      .item-info-column {\n        display: flex;\n        .item-image {\n          display: flex;\n          flex-direction: column;\n          justify-content: center;\n\n          img {\n            width: 45px;\n            height: 68px;\n            border: 1px solid #d5d9e1;\n            border-radius: 2px;\n          }\n        }\n        .item-info {\n          margin-left: 20px;\n          .brand-name {\n            font-size: 12px;\n            color: #6e7787;\n            margin-top: 2px;\n          }\n          .barcode {\n            font-size: 12px;\n            margin-top: 20px;\n            span {\n              color: #6e7787;\n              margin-right: 2px;\n            }\n          }\n        }\n      }\n\n      .collect-item {\n    