`\n      : '-';\n  },\n  numberToCount (value) {\n    return value.toLocaleString('tr-TR');\n  },\n  object2Querystring (options) {\n    return _.reduce(_.keys(options), (str, key, i) => {\n      const delimiter = (i === 0) ? '?' : '&';\n      const k = encodeURIComponent(key);\n      const val = encodeURIComponent(options[k]);\n      return [str, delimiter, k, '=', val].join('');\n    }, '');\n  },\n  absoluteUrl (path, options) {\n    if (options) {\n      const q = this.object2Querystring(options);\n\n      return `${Vue.http.options.root}${path}${q}`;\n    }\n\n    return `${Vue.http.options.root}${path}`;\n  },\n  twoDigits (num) {\n    if (num.toString().length <= 1) {\n      return `0${num.toString()}`;\n    }\n    return num.toString();\n  },\n  lastMonthMilis () {\n    return moment().subtract(1, 'months').valueOf();\n  },\n  priceToNumber (val) {\n    return typeof val === 'string' ? parseFloat(val.replace(',', '.').replace('₺', '').replace(' ', ''), 10) : val;\n  },\n};\n","export default {\n  API_ROOT: process.env.VUE_APP_API_ROOT,\n  IS_PRODUCTION: process.env.NODE_ENV === 'production',\n};\n","import config from '../config';\n\nexport default {\n  setImageFullUrl (url) {\n    const baseApiUrl = config.API_ROOT;\n    return `${baseApiUrl}products/product-image-url/${url}`;\n  },\n\n  /**\n   * Return cdn image url\n   * @param imagePath\n   * @param width\n   * @param height\n   * @returns {string}\n   */\n  generateImgUrl (imagePath, { width, height } = {}) {\n    if (typeof width !== 'undefined' && typeof height !== 'undefined') {\n      return `https://img-trendyol.mncdn.com/mnresize/${width}/${height}${imagePath}`;\n    }\n\n    return `https://img-trendyol.mncdn.com${imagePath}`;\n  },\n  setImagePublicPath (fileName) {\n    const publicPath = process.env.VUE_APP_PUBLIC_PATH;\n    return `${publicPath}assets/images/${fileName}`;\n  },\n};\n","export default {\n  generateAudio (fileName) {\n    const publicPath = process.env.VUE_APP_PUBLIC_PATH;\n  