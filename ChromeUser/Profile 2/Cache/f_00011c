{};\n      this.params.confirmation = false;\n      this.params.description = '';\n      this.isTrySubmit = false;\n    },\n    async getInvoicePreviewData () {\n      this.loading = true;\n      this.hasError = false;\n      try {\n        const orderLineItemIds = Object.values(this.order.orderLines)\n          .map(lineItem => Object.values(lineItem.packageItems))\n          .reduce((res, item) => res.concat(item), [])\n          .map(lineItemId => lineItemId.toString());\n        const orderId = this.order.orderId.toString();\n        const body = {\n          orderId,\n          orderLineItemIds,\n        };\n        this.invoicePreviewData = await MyInvoiceBFF.post('invoice/preview', body, { hideSpinner: true, hideErrorMessage: true });\n\n        this.items = this.invoicePreviewData.products;\n      } catch (e) {\n        this.hasError = true;\n        this.invoicePreviewData = null;\n        this.items = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    async onSubmit () {\n      this.isTrySubmit = true;\n\n      const allValidations = await Promise.all(\n        this.items.map((item, idx) => this.$refs[`${item.name}_${idx}`].$validator.validateAll())\n      );\n      const isValid = allValidations.every(validation => validation);\n\n      if (!isValid || this.exemptionError || this.hasConfirmationError) {\n        return false;\n      }\n      const packageNo = this.order.shipmentPackageDetails.deliveryNumber.toString();\n      this.invoicePreviewData.customer.email = this.order.shipmentAddress.email;\n\n      const body = {\n        ...this.invoicePreviewData,\n        packageNo,\n        shipmentNumber: this.invoicePreviewData.shipmentNumber.toString(),\n        relatedPackages: this.invoicePackageStatus.relatedPackages || [],\n        ...this.hasZeroTaxRate\n          ? {\n            exemptionReason: {\n              ...this.params.exemptionReason,\n              ...this.params.description\n                ? { description: this.params.description }\n                : {},\n            },\n          }\n          : {},\n      };\n\n      if (this.order.shipmentAddress.identityNumber) {\n        body.customer.identityNumber = this.order.shipmentAddress.identityNumber;\n      }\n\n      await MyInvoiceBFF.post('/invoice/create', body);\n      this.onClose();\n    },\n    onClose () {\n      this.$emit('close');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.create-invoice-modal{\n  ::v-deep .mp-checkbox {\n    min-width: 16px;\n  }\n  ::v-deep .mp-select {\n    .mp-select-toggle {\n      padding-left: 12px;\n    }\n    .mp-dropdown {\n      max-height: 175px !important;\n    }\n  }\n  &::v-deep {\n    .modal-dialog{\n      max-width: 666px;\n      .modal-header{\n        padding: 30px;\n      }\n      .modal-body{\n        padding: 0 30px 15px 30px;\n        min-height: 35vh;\n        max-height: 500px;\n        overflow: auto;\n      }\n      .modal-body.center {\n        display: flex;\n        align-items: center;\n        flex-direction: column;\n        justify-content: center;\n      }\n      .modal-footer{\n        padding: 30px;\n      }\n    }\n  }\n  .button-container{\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n\n    .confirmation, .create-invoice {\n      width: calc(50% - 16px);\n    }\n\n    .confirmation {\n      display: flex;\n\n      span {\n        display: block;\n        margin-left: 5px;\n        font-size: 12px;\n        color: #273142;\n        font-weight: normal;\n        cursor: default;\n      }\n    }\n  }\n  .invoice {\n    .table-wrapper {\n      margin-bottom: 30px;\n    }\n\n    .exemption-reason-wrapper {\n      .description {\n        margin-top: 20px;\n\n        &::placeholder {\n          font-size: 12px;\n          color: #9398a0;\n        }\n      }\n    }\n  }\n\n  .error-wrapper{\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n    .icon{\n      font-size: 48px;\