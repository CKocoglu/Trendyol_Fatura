 (data) {\n                const uploadTypeOption = uploadTypeOptions.find((opt) => opt.value === data);\n                return uploadTypeOption ? uploadTypeOption.label : '-';\n              }\n              return '-';\n            },\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.createdAt'),\n            field: 'createdAt',\n            formatter: (data) => data\n              ? dateFormat.date(data, 'DD.MM.YYYY - HH:mm')\n              : '-',\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.finishedAt'),\n            field: 'finishedAt',\n            formatter: (data) => data\n              ? dateFormat.date(data, 'DD.MM.YYYY - HH:mm')\n              : '-',\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.totalOrder'),\n            field: 'totalElements',\n            formatter: (data) => numberFormat.numberToCount(data),\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.errorCount'),\n            field: 'errorCount',\n            formatter: (data) => numberFormat.numberToCount(data),\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.result'),\n            field: 'result',\n            formatter: (data, item) => {\n              if (item.reportDownloadUrl) {\n                return `<a class=\"download-report\" href=\"${item.reportDownloadUrl}\" target=\"_blank\">${this.$t('bulkOrderOperations.body.uploadHistory.downloadReport')}</a>`;\n              }\n              return `<span class=\"download-report no-file\">-</span>`;\n            },\n          },\n        ],\n        body: [],\n        pagination: {\n          page: 0,\n          size: 5,\n        },\n      },\n    };\n  },\n  created () {\n    this.getExcelReports();\n    this.ERROR_MESSAGES = {\n      NOT_MATCH_HEADER: this.$t('bulkOrderOperations.errors.notMatchHeader'),\n    };\n  },\n  computed: {\n    isUploadButtonDisabled () {\n      return !this.selectedFile;\n    },\n    headerGobitoLink () {\n      return RemoteConfigService.getConfigValueByKey('BULK_ORDER_OPERATION_HEADER_GOBITO') || '#';\n    },\n    learnGobitoLink () {\n      return RemoteConfigService.getConfigValueByKey('BULK_ORDER_OPERATION_LEARN_GOBITO') || '#';\n    },\n  },\n  methods: {\n    async getDownloadSampleFilePath () {\n      let { filePath } = await CargoChangeService.downloadSampleFile({ });\n\n      return filePath;\n    },\n    async downloadSampleFile () {\n      let filePath = await this.getDownloadSampleFilePath();\n\n      window.open(filePath, '_blank');\n    },\n    async getExcelReports ({ page = 0 } = { page: 0 }) {\n      let { content, ...pagination } = await CargoChangeService.getExcelReports({ page });\n\n      this.tableConfig.body = content;\n      this.tableConfig.pagination = pagination;\n    },\n    pageChange (page) {\n      this.getExcelReports({ page });\n    },\n    refreshUploadHistory () {\n      this.getExcelReports({ page: this.tableConfig.pagination.page });\n    },\n    async uploadFile () {\n      try {\n        this.$loading.show();\n\n        const excel = await Excel.parse({ file: this.selectedFile });\n        const result = CargoChangeExcelValidator.validate({ excel });\n\n        if (!result.valid) {\n          return this.$toast.error(this.ERROR_MESSAGES[result.error.key], {\n            duration: 5000,\n          });\n        }\n      } finally {\n        this.$loading.hide();\n      }\n\n      let { batchRequestId, ...params } = await CargoChangeService.getUploadUrl({ uploadType: this.uploadType });\n\n      await OssService.uploadFile(this.selectedFile, params);\n      await CargoChangeService.processExcel({ batchRequestId });\n      this.selectedFile = null;\n      await this.getExcelReports();\n      this.$toast.success(this.$t('commons.successMessage'), {\n        duration: 5000,\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .bulk-order-o