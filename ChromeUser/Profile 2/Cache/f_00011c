: SellerBranchService.get(),\n      isTableLoading: false,\n    };\n  },\n  created () {\n    if (this.branch && this.branch.id) {\n      this.params.storeId = this.branch.id;\n    }\n    this.handleTable();\n  },\n  methods: {\n    ...mapActions({\n      getCancels: 'getCancels',\n    }),\n    async handleTable () {\n      if (window.user.storeBaseEnabled && !this.branch.id) {\n        return;\n      }\n      this.isTableLoading = true;\n      const allCancels = await this.getCancels({ params: this.params, config: { hideSpinner: true } });\n      this.tableConfig.hasCustomRow = true;\n\n      this.tableConfig.body = allCancels.content.map((i) => {\n        const item = i;\n\n        let filteredLines = _.filter(item.lines, (l) => {\n          const filteredLine = l;\n          const pItems = l.packageItems;\n          filteredLine.packageItems = _.filter(pItems, p => !_.isNull(p.cancellationReasonName || p.cancellationReason.name));\n          return filteredLine.packageItems.length;\n        });\n\n        const newlines = [];\n        filteredLines.forEach((q) => {\n          const line = q;\n          const newLine = Object.assign({}, line, {});\n\n          const groupedPackage = line.packageItems.reduce((a, val) => {\n            const acc = a;\n            (acc[val.cancellationReasonName || val.cancellationReason.name] = acc[val.cancellationReasonName || val.cancellationReason.name] || []).push(val);\n            return acc;\n          }, {});\n\n          if (Object.keys(groupedPackage).length > 1) {\n            delete groupedPackage[Object.keys(groupedPackage)[0]];\n            Object.keys(groupedPackage).forEach((key) => {\n              const packageItems = groupedPackage[key];\n              newLine.packageItems = packageItems;\n              newlines.push(newLine);\n              line.packageItems = line.packageItems\n                .filter(pItem => packageItems.indexOf(pItem) < 0);\n            });\n          }\n        });\n        filteredLines = f