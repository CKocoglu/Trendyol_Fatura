AlternativeCargoProvider',\n    }),\n    async getCargoProviders () {\n      const providers = await orderService.getRepackageCargoProviders();\n      return providers.data;\n    },\n    async setDefaultCargoProviders () {\n      if (!this.allCargoProviders.length) { this.allCargoProviders = await this.getCargoProviders(); }\n\n      this.cargoProviders = this.allCargoProviders\n        .filter((cp) => [CargoProviders.all.MIGROS.id, CargoProviders.all.TEX_MP.id].indexOf(cp.id) === -1)\n        .map(p => ({\n          id: p.id,\n          name: p.name,\n          text: enums.translate2Turkish(p.name),\n          label: enums.translate2Turkish(p.name),\n        }));\n    },\n    onConfirmSubmit () {\n      this.onSubmitted = true;\n      const hasUrlError = this.getAlternativeUrlHasError;\n      const hasPhoneError = this.getAlternativePhoneHasError;\n\n      if (this.alternativeTrackingInfo.url.length > 250) {\n        this.$toast.error('Kargo Takip Url Adresi 250 karakterden fazla olamaz.', {\n          duration: 5000,\n        });\n      } else if (this.selectedCargoType === this.cargoTypes.alternative && this.selectedAlternativeType === '') {\n        this.$toast.error('Lütfen Alternatif Kargo Türünü Seçiniz.', {\n          duration: 5000,\n        });\n      } else if (this.selectedCargoType === this.cargoTypes.alternative && (hasUrlError || hasPhoneError)) {\n        this.$toast.error('Eksik Parametre, Kargo Takip URL Adresi veya İletişim Bilgisi Boş Olamaz', {\n          duration: 5000,\n        });\n      } else if (this.selectedCargoType === this.cargoTypes.alternative) {\n        this.showConfirmDialog = true;\n      } else if (this.selectedCargoType === this.cargoTypes.default) {\n        this.onSubmit();\n      }\n    },\n    onConfirmCancel () {\n      this.showConfirmDialog = false;\n    },\n    async onSubmit () {\n      const data = {\n        selectedItemId: this.item.id,\n        data: {\n          cargoProvider: this.selectedCargoProvider.name,\n        },\n        cargoProviderName: enums.translate2Turkish(this.selectedCargoProvider.name),\n      };\n\n      if (this.selectedCargoType === this.cargoTypes.default) {\n        await this.changeShipmentCargoProvider(data);\n      } else if (this.selectedCargoType === this.cargoTypes.alternative) {\n        data.cargoProviderName = enums.translate2Turkish('ADEL');\n        data.data.isPhoneNumber = this.selectedAlternativeType === this.alternativeTypes.phone;\n        if (this.selectedAlternativeType === this.alternativeTypes.phone) {\n          data.data.trackingInfo = this.alternativeTrackingInfo.phone;\n        } else {\n          data.data.trackingInfo = this.alternativeTrackingInfo.url;\n        }\n        data.data.instantOrder = window.user.storeBaseEnabled;\n        await this.changeShipmentAlternativeCargoProvider(data);\n      }\n\n      this.$toast.success('İşleminiz başarıyla gerçekleşmiştir.', {\n        duration: 5000,\n      });\n      this.$emit('cargoProviderInfo', data.cargoProviderName);\n\n      this.close();\n    },\n    close () {\n      this.$emit('close');\n      this.onSubmitted = false;\n      this.selectedCargoType = !this.hasCargoProviderChangeAllowed && this.hasAlternativeDeliveryAllowed ? this.cargoTypes.alternative : this.cargoTypes.default;\n      this.selectedCargoProvider = {};\n      this.alternativeTrackingInfo = {\n        phone: '',\n        url: '',\n      };\n    },\n    defaultCargoType () {\n      this.selectedCargoType = this.cargoTypes.default;\n    },\n    alternativeCargoType () {\n      this.selectedCargoType = this.cargoTypes.alternative;\n    },\n    urlAlternativeType () {\n      this.selectedAlternativeType = this.alternativeTypes.url;\n    },\n    phoneAlternativeType () {\n      this.selectedAlternativeType = this.alternativeTypes.phone;\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.mp-link {\n  color: #f27a1a;\n}\n\n.mh-300 {\n  min-height: 300px;\n}\n\n.cargo-type-content {\n  padding: 0 0 20px 24px;\n