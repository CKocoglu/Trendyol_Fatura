Items);\n        for (let i = 0; i < this.orderLinesIds[key]; i++) {\n          const packageItems = this.item.orderLines[key].packageItems;\n          options.splitGroups[0].orderLineItems.push(packageItems[Object.keys(packageItems)[i]]);\n        }\n      });\n      await this.multipleSplitPackage(options);\n      window.scEventBus.publisher('INCREMENT_SHIPMENT_PACKAGE_ITEM_COUNT', {\n        shipmentPackageStatus: 'readyToShipPackageCount',\n      });\n      this.update();\n    },\n    async handleAccept () {\n      if (this.multiSplitEnabled) {\n        await this.handleAcceptMultiSplit();\n      } else {\n        await this.handleAcceptSingleSplit();\n      }\n    },\n  },\n  computed: {\n    isSubmitDisabled () {\n      return Object.keys(this.orderLinesIds).length;\n    },\n    multiSplitEnabled () {\n      return remoteConfig.getConfigValueByKey('ORDER_MULTI_SPLIT_PACKAGE_ENABLED');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  input[type=checkbox] {\n    vertical-align: middle;\n    float: right;\n  }\n\n  label {\n    display: flex;\n    justify-content: space-between;\n    overflow: hidden;\n  }\n\n  .form-group {\n    display: flex;\n    align-items: center;\n  }\n</style>\n","import mod from \"-!../../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../../node_modules/thread-loader/dist/cjs.js!../../../../node_modules/babel-loader/lib/index.js!../../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./SplitPackage.vue?vue&type=script&lang=js&\"; export default mod; export * from \"-!../../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../../node_modules/thread-loader/dist/cjs.js!../../../../node_modules/babel-loader/lib/index.js!../../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./SplitPackage.vue?vue&type=script&lang=js&\"","import { render, staticRenderFns } fr