        />\n      </div>\n    </div>\n  </form>\n</template>\n\n<script>\nimport { mapActions, mapState } from 'vuex';\nimport { tr } from 'vuejs-datepicker/dist/locale';\n\nimport Datepicker from 'vuejs-datepicker';\nimport moment from 'moment';\nimport orders from '@/services/orders';\nimport ScBtnGroupExcel from '@trendyol/sc-ui-kit/ScExcelDownloader';\nimport images from '@/utils/images';\nimport { remoteConfig } from '@/services';\n\nexport default {\n  name: 'FilterBar',\n  components: {\n    ScBtnGroupExcel,\n    Datepicker,\n  },\n\n  props: {\n    showPackageStatuses: {\n      type: Boolean,\n      default: true,\n    },\n\n    showCustomerName: {\n      type: Boolean,\n      default: true,\n    },\n\n    showShipmentPackageIds: {\n      type: Boolean,\n      default: true,\n    },\n  },\n\n  data () {\n    return {\n      statusesPackages: [{\n        name: 'Tümü',\n        data: 'Created,Invoiced,Picking,Shipped,Delivered,Returned,UnDelivered',\n      }, {\n        name: 'Gönderime Hazır',\n        data: 'Created,Invoiced,Picking',\n      }, {\n        name: 'Taşıma Durumunda',\n        data: 'Shipped,UnDelivered',\n      }, {\n        name: 'Teslim Edilen',\n        data: 'Delivered,Returned',\n      }],\n      excelHistories: [],\n      datepickerLang: tr,\n    };\n  },\n  methods: {\n    ...mapActions([\n      'updateFilterParamsByField',\n      'clearFilterParams',\n    ]),\n    handleFilter () {\n      const startDate = this.filterParams.originShipmentStartDate;\n      const endDate = this.filterParams.originShipmentEndDate;\n      if ((startDate && startDate.getFullYear() < 2020) || (endDate && endDate.getFullYear() < 2020)) {\n        this.$toast.error('1 Ocak 2020 öncesine ait siparişler, satıcı paneli performasınızı etkilememesi adına yalnızca Excel formatında indirilebilir olacaktır. Excel İndir seçeneği ile bilgisayarınıza indirebilirsiniz.', {\n          duration: 7000,\n        });\n        return;\n      }\n      this.filterParams.page = 0;\n      this.$emit('handleFilter');\n    },\n    async downloadExcel (callbackFn) {\n      const params = { ...this.filterParams };\n      const startDate = params.originShipmentStartDate;\n      const endDate = params.originShipmentEndDate;\n\n      params.shipmentPackageFulfillmentTypeId = 7;\n      params.originShipmentStartDate = params.originShipmentStartDate ? params.originShipmentStartDate.setHours(0, 0, 0, 0) : '';\n      params.originShipmentEndDate = params.originShipmentEndDate ? params.originShipmentEndDate.setHours(23, 59, 59, 999) : '';\n      params.packageStatuses = params.packageStatuses.split(',');\n      params.shipmentPackageIds = params.shipmentPackageIds.split();\n\n      if (!startDate || !endDate) {\n        this.$toast.error('Lütfen Tarih Aralığı Belirtiniz.', {\n          duration: 5000,\n        });\n      } else {\n        await orders.queueExcel(params);\n        this.$toast.success('İşleminiz başarıyla gerçekleşmiştir.', {\n          duration: 5000,\n        });\n        if (callbackFn instanceof Function) {\n          callbackFn();\n        }\n      }\n    },\n    async getExcelDownloadHistory () {\n      const histories = await orders.getExcelDownloadHistory();\n      this.excelHistories = histories.map(item => {\n        return {\n          fileName: item.fileName,\n          url: item.url,\n          excelStatus: item.supplierExcelStatus,\n          time: item.time,\n        };\n      });\n    },\n    resetForm (e) {\n      e.preventDefault();\n      this.clearFilterParams();\n      this.updateFilterParamsByField({ field: 'packageStatuses', value: this.$route.meta.statuses });\n      this.updateFilterParamsByField({ field: 'type', value: this.$route.meta.type });\n    },\n    onSelectedStartDate (value) {\n      this.updateFilterParamsByField({ field: 'originShipmentStartDate', value });\n    },\n    onSelectedEndDate (value) {\n      this.updateFilterParamsByField({ field: 'originShipmentEndDate', value });\n    },\n  },\n  computed: {\n    ...mapState({\n      filterParams: state => state.orders.filterParams,\n    }),\n    disableStartDate () {\n      const range = {};\n      range.from = this.filterParams.originShipmentEndDate ? moment(this.filterParams.originShipmentEndDate).subtract(1, 'days').toDate() : null;\n      if (this.isExcelEndDate14Days) {\n        range.to = moment(this.filterParams.originShipmentEndDate).subtract(14, 'days').toDate();\n      } else {\n        range.to = moment(this.filterParams.originShipmentEndDate).subtract(7, 'days').toDate();\n      }\n\n      return range;\n    },\n    disableEndDate () {\n      const range = {};\n      range.to = this.filterParams.originShipmentStartDate ? moment(this.filterParams.originShipmentStartDate).add(1, 'days').toDate() : null;\n      if (this.isExcelEndDate14Days) {\n        range.from = moment(this.filterParams.originShipmentStartDate).add(14, 'days').toDate();\n      } else {\n        range.from = moment(this.filterParams.originShipmentStartDate).add(7, 'days').toDate();\n      }\n\n      return range;\n    },\n    closeIconImg () {\n      return images.setImagePublicPath('close-icon.svg');\n    },\n    isExcelEndDate14Days () {\n      return remoteConfig.getConfigValueByKey('IS_EXCEL_END_DATE_14_DAYS');\n    },\n  },\n  created () {\n    if (this.$route.query.orderNumber) {\n      this.updateFilterParamsByField({ field: 'orderNumber', value: this.$route.query.orderNumber });\n    }\n    if (this.$route.query.packageNumber) {\n      this.updateFilterParamsByField({ field: 'shipmentPackageIds', value: this.$route.query.packageNumber });\n    }\n    this.$emit('handleFilter');\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.filter {\n  margin: 20px 0;\n\n  &-body {\n    padding: 10px;\n    margin: 0;\n    background-color: #ffffff;\n    border-top: 0;\n\n    .form-group {\n      padding: 10px;\n      margin-bottom: 0;\n\n      label {\n        font-size: 10px;\n        font-weight: 500;\n        color: #6e7787;\n      }\n