CreateInvoiceButton: false,\n      isInreview: false,\n      isNotApplyBefore: false,\n      isOpenDropdown: false,\n      loading: false,\n      invoicePackageStatus: {},\n      cancelButton: {\n        show: false,\n        text: 'FATURA İPTAL ET',\n        disabled: false,\n      },\n      supplierStatus: '',\n    };\n  },\n\n  computed: {\n    canUseMyInvoice () {\n      const supplier = SupplierService.getSupplier();\n\n      return (!this.supplierStatus || this.supplierStatus === 'APPROVED') &&\n        !supplier.mealSeller &&\n        !supplier.storeBaseEnabled;\n    },\n  },\n\n  mounted () {\n    window.scEventBus.subscribe('MY_INVOICE_SUPPLIER_STATUS', (status) => {\n      this.supplierStatus = status;\n      this.isNotApplyBefore = status === 'NO_APPLICATION';\n      this.isInreview = status === 'INREVIEW';\n      this.showCreateInvoiceButton = this.isInreview;\n    });\n  },\n\n  methods: {\n    onInvoiceCreate () {\n      if (!this.isInreview) {\n        this.onClose();\n        this.$emit('invoiceCreate', {\n          invoicePackageStatus: this.invoicePackageStatus,\n        });\n      }\n    },\n    onInvoiceView () {\n      this.onClose();\n      this.$emit('invoiceView');\n    },\n    onApplyMyInvoice () {\n      this.onClose();\n      this.$emit('applyMyInvoice');\n    },\n    onCancelInvoice () {\n      this.onClose();\n      this.$emit('cancelInvoice');\n    },\n    showInvoiceInfoModal () {\n      this.onClose();\n      this.$emit('showInvoiceInfoModal');\n    },\n    onClose () {\n      this.isOpenDropdown = false;\n    },\n    async toggleDropdown () {\n      this.isOpenDropdown = !this.isOpenDropdown;\n\n      if (this.isOpenDropdown && this.canUseMyInvoice) {\n        this.showCreateInvoiceButton = false;\n        this.showViewInvoiceButton = false;\n        this.invoiceCreated = false;\n        this.loading = false;\n\n        try {\n          this.loading = true;\n          const packageNo = this.order.shipmentPackageDetails.deliveryNumber;\n          const invoicePackageStatus = await MyInvoiceBFF.get(`/invoice/package/status`, {\n            params: {\n              packageNo,\n              orderId: this.order.orderId,\n            },\n            hideSpinner: true,\n          });\n          this.invoicePackageStatus = invoicePackageStatus;\n\n          let {\n            supplierStatus,\n            invoiceStatus,\n          } = invoicePackageStatus;\n\n          if (!this.supplierStatus) {\n            window.scEventBus.publish('MY_INVOICE_SUPPLIER_STATUS', supplierStatus);\n          }\n\n          if (supplierStatus === 'NO_APPLICATION') {\n            this.isNotApplyBefore = true;\n          } else if (supplierStatus === 'INREVIEW') {\n            this.showCreateInvoiceButton = true;\n            this.isInreview = true;\n          } else if (supplierStatus === 'APPROVED') {\n            if (['COMPLETED', 'INITIATED', 'RETRYING_SEND'].indexOf(invoiceStatus) > -1) {\n              this.showViewInvoiceButton = true;\n              this.invoiceCreated = invoiceStatus === 'COMPLETED';\n            } else if (['NOTINITATED'].indexOf(invoiceStatus) > -1) {\n              this.showCreateInvoiceButton = true;\n            }\n            this.determineCancelButtonVisibility(invoiceStatus);\n          }\n        } catch (e) {\n          this.isOpenDropdown = true;\n        } finally {\n          this.loading = false;\n        }\n      }\n    },\n    determineCancelButtonVisibility (invoiceStatus) {\n      const CANCEL_BUTTON_STATUS = {\n        CANCELLING: 'CANCELLING',\n        RETRYING_CANCEL: 'RETRYING_CANCEL',\n        CANCELLED: 'CANCELLED',\n        COMPLETED: 'COMPLETED',\n      };\n      let buttonText = 'FATURA İPTAL ET';\n\n      if (invoiceStatus === CANCEL_BUTTON_STATUS.CANCELLING || invoiceStatus === CANCEL_BUTTON_STATUS.RETRYING_CANCEL) {\n        buttonText = 'İPTAL EDİLİYOR...';\n      }\n      if (invoiceStatus === CANCEL_BUTTON_STATUS.CANCELLED) {\n        buttonText = 'İPTAL EDİLDİ';\n      }\n\n      this.cancelButton.disabled = buttonText !== 'FATURA İPTAL ET';\n      this.cancelButton.show = remoteConfig.getConfigValueByKey('APPLY_EF_161', false) &&\n        Object.values(CANCEL_BUTTON_STATUS).includes(invoiceStatus);\n      this.cancelButton.text = buttonText;\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.dropdown-container {\n  display: inline-block;\n  position: relative;\n  font-size: 14px;\n  font-weight: normal;\n  color: #6e7787;\n\n  .dropdown{\n    margin-bottom: 0;\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    background-color: #f1f2f7;\n    color: #273142;\n    border-radius: 5px;\n    padding: 5px 10px;\n    height: 34px;\n    white-space: nowrap;\n\n    span.icon-arrow-down{\n      font-size: 6px;\n      margin-left: 10px;\n    }\n  }\n\n  .dropdown-body {\n    position: absolute;\n    margin-top: 5px;\n    border: 1px solid #f27a1a;\n    width: 290px;\n    border-radius: 8px;\n    background-color: #ffffff;\n    padding: 15px;\n    box-shadow: 0 16px 24px -8px rgba(149, 161, 181, 0.25);\n    z-index: 12;\n  }\n\n  .logo{\n    width: 124px;\n    height: 27px;\n    object-fit: contain;\n  }\n  .status-info-text{\n    margin: 15px 0;\n    font-size: 14px;\n    color: #0bc15c;\n  }\n  .apply-info-text{\n    margin: 15px 0;\n    font-size: 14px;\n    color: #273142;\n  }\n\n  .buttons-wrapper {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    color: #273142;\n\n    .primary-button{\n      margin-bottom: 15px;\n    }\n\n    .primary-button, .secondary-button{\n      height: 34px;\n      padding: 0;\n\n      &:not(:last-child) {\n        margin-bottom: 10px;\n      }\n\n      &.disabled{\n        border: solid 1px #d5d9e1;\n        background-color: #ffffff;\n        font-weight: 500;\n        color: #afbbca;\n        cursor: not-allowed;\n      }\n    }\n  }\n\n  ::v-deep .loading-dots {\n    height: 100px;\n    display: flex;\n    align-items: center;\n    j