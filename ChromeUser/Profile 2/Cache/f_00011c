Required) {\n        this.provisionModal = true;\n      } else {\n        this.setInvoiced();\n      }\n    },\n    async setInvoiced (amount) {\n      await instant.setInvoiced(this.orders.selectedItem.id, amount);\n      this.$toast.success(this.$t('commons.successMessage'), {\n        duration: 5000,\n      });\n      setTimeout(() => {\n        this.handleTable();\n      }, 1000);\n    },\n    showInstantEditModal () {\n      if (this.isInstantUser) {\n        window.scrollTo(0, 0);\n        this.instantEditSummaryItems = [];\n        this.instantEditModal = true;\n      }\n    },\n    showInstantEditSummaryModal (items) {\n      if (this.isInstantUser) {\n        window.scrollTo(0, 0);\n        this.instantEditSummaryModal = true;\n        this.instantEditSummaryItems = [...items];\n      }\n    },\n    cargoProviderInfo (cargo) {\n      if (cargo === CargoProviders.all.UPS_MP.name) {\n        this.cargoProviderChangeInfoText = 'Barkod yazdırmak için koli adedini girmeyi unutma';\n        this.cargoProviderChangeInfoModal = true;\n      } else if (cargo === CargoProviders.all.HOROZ_MP.name ||\n        cargo === CargoProviders.all.CEVA_MP.name ||\n        cargo === CargoProviders.all.BOR_MP.name) {\n        this.cargoProviderChangeInfoText = 'Barkod yazdırmak için desi ve koli adedini girmeyi unutma';\n        this.cargoProviderChangeInfoModal = true;\n      }\n    },\n  },\n  computed: {\n    ...mapState([\n      'orders',\n    ]),\n    ...mapGetters(['orderNotifications']),\n    isProvisionRequired () {\n      return this.orders.selectedItem.provision === ProvisionStatus.REQUIRED;\n    },\n    showBoxQuantity () {\n      const shouldHaveCargoProviders = [CargoProviders.all.UPS_MP.id];\n      const hasUPS = shouldHaveCargoProviders\n        .indexOf(this.orders.selectedItem.shipmentPackageDetails.cargoProviderId) !== -1;\n\n      return (this.orders.selectedItem.whoPays === null || this.orders.selectedItem.whoPays === 0) || !hasUPS;\n    },\n    showDeci () {\n