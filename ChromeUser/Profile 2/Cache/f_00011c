Mask = {\n  bind: (el, binding) => {\n    const props = maskList[binding.value] || {\n      mask: binding.value,\n    };\n\n    props['autoUnmask'] = binding.arg === 'autoUnmask';\n    props['showMaskOnFocus'] = true;\n    props['showMaskOnHover'] = true;\n    Inputmask(props)\n      .mask(el);\n  },\n};\n\nexport default VueInputMask;\n\nVue.directive('mask', VueInputMask);\n","<template>\n  <ScModal\n    :show=\"show\"\n    @close=\"close\"\n    :class-name=\"'large'\"\n  >\n    <form\n      @submit.prevent=\"onConfirmSubmit\"\n      class=\"form-horizontal\"\n    >\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\">\n          Paketi hangi seçenek ile göndermek istiyorsunuz?\n        </h5>\n        <button\n          type=\"button\"\n          class=\"close\"\n          @click=\"close()\"\n        >\n          <span>&times;</span>\n        </button>\n      </div>\n      <div class=\"modal-body mh-300\">\n        <div\n          class=\"row\"\n          v-if=\"hasCargoProviderChangeAllowed\"\n        >\n          <div class=\"col-md-12\">\n            <ScRadio\n              css-class=\"mp-radio-2\"\n              value=\"default\"\n              name=\"cargo_type\"\n              @input=\"defaultCargoType\"\n              v-model=\"selectedCargoType\"\n              ref=\"default\"\n            >\n              Trendyol anlaşmalı farklı kargo firması ile göndermek istiyorum\n            </ScRadio>\n            <div\n              class=\"cargo-type-content\"\n              ref=\"cargoTypeContent0\"\n              v-if=\"selectedCargoType === cargoTypes.default\"\n            >\n              <a\n                class=\"mp-link\"\n                href=\"https://partnerakademi.trendyol.com/mp/#kargo\"\n                target=\"_blank\"\n              >\n                Anlaşmalı\n                olduğumuz kargo firmalarına ulaşmak için tıklayınız\n              </a>\n              <p class=\"text-muted\">\n                Değiştirmek istediğiniz