\n        instantOrder: window.user.storeBaseEnabled,\n      };\n\n      _.each(this.orderLinesIds, (quantity, lineId) => {\n        const pIds = Object.keys(this.item.orderLines[lineId].packageItems)\n          .slice(0, quantity);\n        options.packageItemIds = options.packageItemIds.concat(pIds);\n        options.params[lineId] = pIds;\n      });\n      try {\n        await this.unSupplied({ ...options, ignoredResponseCodes: [429] });\n        this.$toast.success('İşleminiz başarıyla gerçekleşmiştir.', {\n          duration: 5000,\n        });\n\n        if (Object.keys(this.item.orderLines).length <= 0) {\n          window.scEventBus.publisher('DECREMENT_SHIPMENT_PACKAGE_ITEM_COUNT', { shipmentPackageStatus: 'readyToShipPackageCount' });\n        }\n\n        this.trackEvent();\n        this.close();\n      } catch (error) {\n        if (error.response.status === 429) {\n          this.showCannotUnSuppliedMessage = true;\n        }\n      }\n    },\n    trackEvent () {\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        category: 'PartnerPortal',\n        action: 'CancelItem',\n        label: this.item.orderNumber,\n      });\n    },\n    async onPreSubmit () {\n      this.showConfirmModal = true;\n      this.$nextTick(() => {\n        document.body.classList.add('modal-open');\n      });\n    },\n  },\n  computed: {\n    isSubmitDisabled () {\n      return !Object.keys(this.orderLinesIds).length > 0;\n    },\n    isInstantSeller () {\n      return window.user.storeBaseEnabled;\n    },\n    title () {\n      return this.showCannotUnSuppliedMessage ? 'Dikkat !' : 'İptal Etmek İstediğiniz Ürünleri Seçiniz';\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n  .form-header {\n    padding-bottom: 10px;\n    border-bottom: 1px solid #d5d9e1;\n  }\n  .col-form-label {\n    display: flex;\n    align-items: center;\n    padding-left: 0;\n    padding-right: 0;\n  }\n  .col-form-select {\n    padding: 0;\n  }\n  .message {\n    margin-bottom: 30p