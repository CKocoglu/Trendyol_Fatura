            status: 'Reddedilme',\n            action: 'reddedildi',\n          };\n          break;\n        case 'Unresolved':\n          text = {\n            status: 'İhtilaf',\n            action: 'henüz nihai karara <u>ulaşmadı</u>',\n          };\n          break;\n        default:\n          text = {\n            status: '',\n            action: '',\n          };\n      }\n\n      if (!claimItem.lastActionDate && !claimItem.lastActionBy) {\n        service.getClaimItemAudits(claimItem.claimItemId).then((res) => {\n          claimItem.lastActionDate = res.lastActionDate;\n          claimItem.lastActionBy = res.lastActionBy;\n\n          this.$forceUpdate();\n          return this.getClaimDetailsText(selectedClaimsOrderDetails, text, reason, claimItem);\n        });\n      }\n\n      return this.getClaimDetailsText(selectedClaimsOrderDetails, text, reason, claimItem);\n    },\n    getClaimDetailsText (selectedClaimsOrderDetails, text, reason, claimItem) {\n      return (`${selectedClaimsOrderDetails.productName} - ${selectedClaimsOrderDetails.barcode} -\n           ${selectedClaimsOrderDetails.productColor || 'No Color'} - ${selectedClaimsOrderDetails.productSize || 'One Size'}</br>` +\n          `${text.status} Tarihi: ${format.date(claimItem.lastActionDate)}</br> İade <u>${this.getLastActionBy(claimItem.lastActionBy)}</u> ${text.action}.</br> ${reason || ''}`);\n    },\n    getLastActionBy (lastActionBy) {\n      const lastActionBytext = {\n        TrendyolAdmin: 'Trendyol tarafından',\n        TrendyolAuto: 'Sistem tarafından otomatik',\n        Supplier: 'Satıcı tarafından',\n        DEFAULT: '',\n      };\n\n      return lastActionBytext[lastActionBy] || lastActionBytext.DEFAULT;\n    },\n    close () {\n      this.$emit('close');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  ul {\n    margin-bottom: 0;\n    padding-left: 2rem;\n\n    li {\n      &:not(:last-child) {\n        margin-bottom: 1rem;\n      }\n    }\n  }\n</style>\n","import