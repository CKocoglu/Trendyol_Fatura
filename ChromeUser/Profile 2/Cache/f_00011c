              v-for=\"(item, index) in (row.packageItemsCount - row._unSuppliedItemsCount)\"\n                :key=\"index\"\n                :value=\"index + 1\"\n              >\n                {{ index + 1 }} Adet\n              </option>\n            </select>\n          </template>\n          <template v-slot:collectedItem=\"{ row }\">\n            <div class=\"collect-item\">\n              <span>Ürün Toplandı</span>\n              <GCheckbox\n                class=\"form-checkbox\"\n                v-model=\"row._isCollected\"\n                @change=\"onCollectedChange(row, $event)\"\n              />\n            </div>\n          </template>\n        </GDataTable>\n      </div>\n      <div class=\"modal-footer\">\n        <GButton\n          variant=\"danger\"\n          @click=\"onCancel\"\n        >\n          TÜM SİPARİŞİ İPTAL ET\n        </GButton>\n        <GButton\n          variant=\"success\"\n          :disabled=\"!isSaveButtonEnabled\"\n          @click=\"showConfirmModal\"\n        >\n          ONAYLA VE KAYDET\n        </GButton>\n      </div>\n    </ScModal>\n    <ScModal\n      class=\"confirm-modal\"\n      :show=\"showUnSuppliedConfirmModal\"\n    >\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\">\n          Sipariş İptal / Alternatif\n        </h5>\n        <button\n          type=\"button\"\n          class=\"close\"\n          @click=\"showUnSuppliedConfirmModal = false\"\n        >\n          <img :src=\"$images.setImagePublicPath('close-btn.png')\">\n        </button>\n      </div>\n      <div class=\"modal-body\">\n        <div class=\"body-container\">\n          <img :src=\"$images.setImagePublicPath('market-siparis-iptal@2x.png')\">\n          <p>Siparişi iptal veya değiştirmek üzeresiniz,<br><b>Müşteriyi bilgilendirdiniz mi?</b></p>\n        </div>\n        <GButton\n          variant=\"primary\"\n          fluid\n          @click=\"onSubmit\"\n        >\n          EVET, MÜŞTERİYİ BİLGİLENDİRDİM\n        