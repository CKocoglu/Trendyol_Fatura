export default {\n  name: 'CreateInvoiceModal',\n  components: {\n    EditableView,\n    ScModal,\n    ScButton,\n    GDataTable,\n    ScThreeDots,\n    ScCheckbox,\n    ScSelect,\n  },\n  props: {\n    show: {\n      type: Boolean,\n      default: false,\n    },\n    order: {\n      type: Object,\n      default: () => ({}),\n    },\n    invoicePackageStatus: {\n      type: Object,\n      default: () => ({}),\n    },\n  },\n  data () {\n    return {\n      loading: false,\n      hasError: false,\n      logoUrl: 'https://cdn.dsmcdn.com/seller-center/my-invoice/images/trendyol-dark.svg',\n      headers: [\n        { text: 'Açıklama', key: 'name' },\n        { text: 'KDV Oranı(%)', key: 'taxRate', align: 'center', formatter: (val) => `%${val}` },\n        { text: 'KDV Oranı(₺)', key: 'totalTaxAmount', align: 'center', formatter: (val) => `${val} ₺` },\n        { text: 'Adet', key: 'quantity', align: 'center' },\n        { text: 'Birim Fiyat', key: 'amount', align: 'center', minWidth: '70px', formatter: (val) => `${val} ₺` },\n        { text: 'Toplam Tutar', key: 'totalAmount', align: 'center', minWidth: '70px', formatter: (val) => `${val} ₺` },\n      ],\n      items: [],\n      invoicePreviewData: null,\n      params: {\n        confirmation: false,\n        exemptionReason: {},\n        description: '',\n      },\n      isTrySubmit: false,\n    };\n  },\n  computed: {\n    exemptionReasons () {\n      const items = [\n        { key: 201, value: '17/1 Kültür ve eğitim amacı taşıyan işlemler' },\n        { key: 202, value: '202 17/2-a Sağlık, çevre ve sosyal yardım amaçlı işlemler ' },\n        { key: 206, value: '17/2-e Mesleki kuruluşların işlemleri' },\n        { key: 212, value: '17/4-ı Serbest bölgelerde verilen hizmetler' },\n        { key: 220, value: 'Geçici 10 Kurumlar Vergisi Kanununun Geçici 28 ve 29. maddeleri kapsamındaki teslimler' },\n        { key: 223, value: 'Geçici 20/1 Teknoloji geliştirme bölgelerinde yapılan işlemler' },\n        { key: 227, value