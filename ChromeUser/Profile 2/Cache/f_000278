        if (!result.valid) {\n          return this.$toast.error(this.ERROR_MESSAGES[result.error.key], {\n            duration: 5000,\n          });\n        }\n      } finally {\n        this.$loading.hide();\n      }\n\n      let { batchRequestId, ...params } = await CargoChangeService.getUploadUrl({ uploadType: this.uploadType });\n\n      await OssService.uploadFile(this.selectedFile, params);\n      await CargoChangeService.processExcel({ batchRequestId });\n      this.selectedFile = null;\n      await this.getExcelReports();\n      this.$toast.success(this.$t('commons.successMessage'), {\n        duration: 5000,\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .bulk-order-operations {\n    max-width: 1366px;\n    background-color: #f1f2f7;\n    padding: 8px 32px;\n    margin: auto;\n\n    & .header {\n      display: flex;\n      justify-content: space-between;\n      padding-bottom: 2px;\n\n      .left {\n        & .title {\n          font-size: 30px;\n          font-weight: 300;\n          font-stretch: normal;\n          font-style: normal;\n          line-height: normal;\n          letter-spacing: normal;\n          color: #273142;\n        }\n\n        .icon-exclamation-circle {\n          font-size: 18px;\n        }\n      }\n    }\n\n    & .body {\n      display: flex;\n      flex-direction: column;\n\n      & .update-order-container {\n        display: flex;\n      }\n\n      & .sc-card-container {\n        padding: 30px 0;\n        flex: 1;\n\n        ::v-deep .sc-card {\n          height: 100%;\n        }\n\n        & .card-body {\n          display: flex;\n          flex-direction: column;\n\n          & > div:not(.upload-history-table) {\n            display: flex;\n            align-items: center;\n\n            &:not(:first-child) {\n              margin-top: 30px;\n            }\n\n            & .badge {\n              width: 90px;\n              height: 90px;\n              min-width: 90px;\n              background-co