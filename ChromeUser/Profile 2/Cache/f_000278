rams = { ...this.filterParams };\n      const startDate = params.originShipmentStartDate;\n      const endDate = params.originShipmentEndDate;\n\n      params.shipmentPackageFulfillmentTypeId = 7;\n      params.originShipmentStartDate = params.originShipmentStartDate ? params.originShipmentStartDate.setHours(0, 0, 0, 0) : '';\n      params.originShipmentEndDate = params.originShipmentEndDate ? params.originShipmentEndDate.setHours(23, 59, 59, 999) : '';\n      params.packageStatuses = params.packageStatuses.split(',');\n      params.shipmentPackageIds = params.shipmentPackageIds.split();\n\n      if (!startDate || !endDate) {\n        this.$toast.error('Ltfen Tarih Aral Belirtiniz.', {\n          duration: 5000,\n        });\n      } else {\n        await orders.queueExcel(params);\n        this.$toast.success('leminiz baaryla gereklemitir.', {\n          duration: 5000,\n        });\n        if (callbackFn instanceof Function) {\n          callbackFn();\n        }\n      }\n    },\n    async getExcelDownloadHistory () {\n      const histories = await orders.getExcelDownloadHistory();\n      this.excelHistories = histories.map(item => {\n        return {\n          fileName: item.fileName,\n          url: item.url,\n          excelStatus: item.supplierExcelStatus,\n          time: item.time,\n        };\n      });\n    },\n    resetForm (e) {\n      e.preventDefault();\n      this.clearFilterParams();\n      this.updateFilterParamsByField({ field: 'packageStatuses', value: this.$route.meta.statuses });\n      this.updateFilterParamsByField({ field: 'type', value: this.$route.meta.type });\n    },\n    onSelectedStartDate (value) {\n      this.updateFilterParamsByField({ field: 'originShipmentStartDate', value });\n    },\n    onSelectedEndDate (value) {\n      this.updateFilterParamsByField({ field: 'originShipmentEndDate', value });\n    },\n  },\n  computed: {\n    ...mapState({\n      filterParams: state => state.orders.filterParams,\n    }),\n    disableStartDat