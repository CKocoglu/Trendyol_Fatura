  } = invoicePackageStatus;\n\n          if (!this.supplierStatus) {\n            window.scEventBus.publish('MY_INVOICE_SUPPLIER_STATUS', supplierStatus);\n          }\n\n          if (supplierStatus === 'NO_APPLICATION') {\n            this.isNotApplyBefore = true;\n          } else if (supplierStatus === 'INREVIEW') {\n            this.showCreateInvoiceButton = true;\n            this.isInreview = true;\n          } else if (supplierStatus === 'APPROVED') {\n            if (['COMPLETED', 'INITIATED', 'RETRYING_SEND'].indexOf(invoiceStatus) > -1) {\n              this.showViewInvoiceButton = true;\n              this.invoiceCreated = invoiceStatus === 'COMPLETED';\n            } else if (['NOTINITATED'].indexOf(invoiceStatus) > -1) {\n              this.showCreateInvoiceButton = true;\n            }\n            this.determineCancelButtonVisibility(invoiceStatus);\n          }\n        } catch (e) {\n          this.isOpenDropdown = true;\n        } finally {\n          this.loading = false;\n        }\n      }\n    },\n    determineCancelButtonVisibility (invoiceStatus) {\n      const CANCEL_BUTTON_STATUS = {\n        CANCELLING: 'CANCELLING',\n        RETRYING_CANCEL: 'RETRYING_CANCEL',\n        CANCELLED: 'CANCELLED',\n        COMPLETED: 'COMPLETED',\n      };\n      let buttonText = 'FATURA PTAL ET';\n\n      if (invoiceStatus === CANCEL_BUTTON_STATUS.CANCELLING || invoiceStatus === CANCEL_BUTTON_STATUS.RETRYING_CANCEL) {\n        buttonText = 'PTAL EDLYOR...';\n      }\n      if (invoiceStatus === CANCEL_BUTTON_STATUS.CANCELLED) {\n        buttonText = 'PTAL EDLD';\n      }\n\n      this.cancelButton.disabled = buttonText !== 'FATURA PTAL ET';\n      this.cancelButton.show = remoteConfig.getConfigValueByKey('APPLY_EF_161', false) &&\n        Object.values(CANCEL_BUTTON_STATUS).includes(invoiceStatus);\n      this.cancelButton.text = buttonText;\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.dropdown-container {\n  display: inline-block;\n  positio