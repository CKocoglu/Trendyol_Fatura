     duration: 5000,\n            });\n\n            return;\n          }\n        }\n        this.selectedOrders.push(claim);\n        this.barcodeType = type;\n        this.barcodeModal = true;\n      }\n\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: GAEvents.CATEGORY.CLAIM_SC,\n        eventAction: type === BarcodeType.ZPL ? GAEvents.ACTION.LABEL_PRINT_ZPL : GAEvents.ACTION.LABEL_PRINT_A4,\n        eventLabel: this.claims.selectedItem.orderNumber,\n      });\n    },\n    async deciBoxQuantity () {\n      const orderNumber = this.claims.selectedItem.orderNumber;\n      const options = {\n        orderNumber,\n      };\n      const order = await orders.getShipmentPackages(options);\n\n      this.updateBoxQuantityAndDeci = false;\n\n      if (order.content) {\n        const item = order.content.find(item => item.orderDetail.orderNumber === orderNumber);\n        const shipmentPackageFilterDto = this.claims.selectedItem.shipmentPackageFilterDto;\n\n        if ((shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.CEVA_MP.id ||\n        shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.HOROZ_MP.id ||\n        shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.BOR_MP.id) &&\n        (item.whoPays === null || item.whoPays === 0)) {\n          this.cargoBoxQuantityDeciModal = true;\n          this.updateBoxQuantityAndDeci = true;\n\n          const options = {\n            boxQuantity: this.cargoBoxQuantity,\n            deci: this.cargoDeci,\n          };\n          await orders.changeDeciBoxQuantity(shipmentPackageFilterDto.id, options);\n\n          if (this.updateBoxQuantityAndDeci) {\n            spinner.start();\n\n            setTimeout(async () => {\n              await this.handleTable();\n\n              spinner.done();\n            }, 2000);\n          }\n        }\n        if (shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.UPS_MP.id &&\n      (item.whoPays === null || it