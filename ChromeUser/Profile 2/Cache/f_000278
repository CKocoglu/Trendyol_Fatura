      columns: [\n          {\n            label: this.$t('bulkOrderOperations.columns.status'),\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.file'),\n            field: 'batchRequestId',\n            formatter: (data, item) => {\n              if (item.excelFileDownloadUrl) { return `<a class=\"filename\" href=\"${item.excelFileDownloadUrl}\" target=\"_blank\" title=\"${data}\">${data}</a>`; }\n              return `<span class=\"filename no-file\">${data}</span>`;\n            },\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.uploadType'),\n            field: 'batchRequestType',\n            formatter: data => {\n              if (data) {\n                const uploadTypeOption = uploadTypeOptions.find((opt) => opt.value === data);\n                return uploadTypeOption ? uploadTypeOption.label : '-';\n              }\n              return '-';\n            },\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.createdAt'),\n            field: 'createdAt',\n            formatter: (data) => data\n              ? dateFormat.date(data, 'DD.MM.YYYY - HH:mm')\n              : '-',\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.finishedAt'),\n            field: 'finishedAt',\n            formatter: (data) => data\n              ? dateFormat.date(data, 'DD.MM.YYYY - HH:mm')\n              : '-',\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.totalOrder'),\n            field: 'totalElements',\n            formatter: (data) => numberFormat.numberToCount(data),\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.errorCount'),\n            field: 'errorCount',\n            formatter: (data) => numberFormat.numberToCount(data),\n          },\n          {\n            label: this.$t('bulkOrderOperations.columns.result'),\n            field: 'result',\n            f