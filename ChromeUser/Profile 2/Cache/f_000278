on') }}\n      </p>\n    </div>\n  </td>\n</template>\n\n<script>\nimport { mapMutations } from 'vuex';\nimport ScDropdown from '@trendyol/sc-ui-kit/ScDropdown';\nimport ScButton from '@trendyol/sc-ui-kit/ScButton';\nimport OrderStatus from '@/enums/Packages/Orders/OrderStatus';\nimport OrderTypes from '@/enums/Packages/Orders/OrderTypes';\nimport CargoProviders from '@/models/CargoProviders';\nimport BarcodeType from '@/enums/BarcodeType';\nimport { remoteConfig } from '@/services';\n\nexport default {\n  name: 'Actions',\n  props: {\n    order: {\n      type: Object,\n      default: () => {},\n    },\n  },\n  components: {\n    ScDropdown,\n    ScButton,\n  },\n  data () {\n    return {\n      BarcodeType: BarcodeType,\n    };\n  },\n  methods: {\n    ...mapMutations(['setSelectedItem']),\n    showWarning () {\n      this.$toast.error('Bu sipari iin kargo takip linki bulunamamtr.', {\n        duration: 5000,\n      });\n    },\n    emitter (name, value) {\n      this.setSelectedItem(this.order);\n      this.$emit(name, value);\n    },\n  },\n  computed: {\n    instantInvoicedButtonEnabled () {\n      const isAlternative = Object.keys(this.order.orderLines).some(key => this.order.orderLines[key].alternativePackageItemCount > 0);\n      const isCollected = Object.keys(this.order.orderLines).some(key => !!this.order.orderLines[key].isCollected);\n      return this.status === 'Picking' &&\n        this.isInstantDeliveryPackages &&\n        this.instantProvisionEnabled &&\n        (isAlternative || isCollected);\n    },\n    multiSplitEnabled () {\n      return remoteConfig.getConfigValueByKey('ORDER_MULTI_SPLIT_PACKAGE_ENABLED');\n    },\n    instantProvisionEnabled () {\n      return remoteConfig.getConfigValueByKey('INSTANT_PROVISION_ENABLED');\n    },\n    instantOrderEditEnabled () {\n      return remoteConfig.getConfigValueByKey('INSTANT_ORDER_EDIT_ENABLED');\n    },\n    status () {\n      return this.order.orderStatus;\n    },\n    isTypeDefectiveOutbound () {\n      const routeMetaType = this.$route.meta.type;\n      return routeMetaType === OrderTypes.TYPE_DEFECTIVE_OUTBOUND;\n    },\n    isPackageSpliteable () {\n      const status = this.status;\n      const hasRole = this.$store.getters.shipmentSplitBySupplierEnabled;\n      const orderStatusIsValid =\n        status === OrderStatus.STATUS_PICKING ||\n        status === OrderStatus.STATUS_READYTOSHIP ||\n        status === OrderStatus.STATUS_INVOICED;\n      const lineCounts = this.multiSplitEnabled ? this.order.totalPackageItemCounts : this.order.orderLinesCount;\n      const hasMultipleLine = lineCounts > 1;\n\n      return hasRole && orderStatusIsValid && hasMultipleLine;\n    },\n    isTakeToActionVisible () {\n      const status = this.status;\n      return (\n        status === OrderStatus.STATUS_CREATED ||\n        status === OrderStatus.STATUS_READYTOSHIP\n      );\n    },\n    isCargoTrackingButtonVisible () {\n      const status = this.status;\n      return (\n        status === OrderStatus.STATUS_SHIPPED ||\n        status === OrderStatus.STATUS_UNDELIVERED ||\n        status === OrderStatus.STATUS_DELIVERED ||\n        status === OrderStatus.STATUS_UNDELIVEREDANDRETURNED ||\n        status === OrderStatus.STATUS_RETURNED ||\n        status === OrderStatus.STATUS_RETURN_SHIPPED\n      );\n    },\n    isCargoProviderChangeButtonVisible () {\n      const hasRole =\n        this.$store.getters.cargoProviderChangeAllowed ||\n        this.$store.getters.alternativeDeliveryAllowed;\n      return this.isNew && hasRole;\n    },\n    isNew () {\n      const status = this.status;\n      return (\n        status === OrderStatus.STATUS_CREATED ||\n        status === OrderStatus.STATUS_READYTOSHIP ||\n        status === OrderStatus.STATUS_PICKING ||\n        status === OrderStatus.STATUS_INVOICED\n      );\n    },\n    isModalAlternativeCargoProvider () {\n      return this.$store.getters.alternativeDeliveryAllowed;\n    },\n    isAlternativeDelivery () {\n      const status = this.status;\n      return (\n        this.isModalAlternativeCargoProvider &&\n        (status === OrderStatus.STATUS_SHIPPED ||\n          status === OrderStatus.STATUS_UNDELIVERED) &&\n        this.order.shipmentPackageDetails.cargoProviderId ===\n          CargoProviders.all.ADEL.id\n      );\n    },\n    isDeciBoxQuantityVisible () {\n      const whoPays = this.order.whoPays;\n      const shipmentPackageDetails = this.order.shipmentPackageDetails;\n\n      return (\n        ((shipmentPackageDetails.cargoProviderId === CargoProviders.all.CEVA_MP.id ||\n        shipmentPackageDetails.cargoProviderId === CargoProviders.all.HOROZ_MP.id ||\n        shipmentPackageDetails.cargoProviderId === CargoProviders.all.BOR_MP.id) &&\n        (shipmentPackageDetails.boxQuantity === null && shipmentPackageDetails.deci === null) &&\n        (whoPays === null || whoPays === 0)) ||\n        (shipmentPackageDetails.cargoProviderId === CargoProviders.all.UPS_MP.id &&\n        shipmentPackageDetails.boxQuantity === null &&\n        (whoPays === null || whoPays === 0))\n      );\n    },\n    isInstantDeliveryPackages () {\n      return window.user.storeBaseEnabled;\n    },\n    isUpsCargoProvider () {\n      const shipmentPackageDetails = this.order.shipmentPackageDetails;\n      return shipmentPackageDetails.cargoProviderId === CargoProviders.all.UPS_MP.id;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.actions-column {\n  width: 200px;\n}\n\n.dropdown-button {\n  margin-top: 15px;\n  display: block;\n  color: #f27a1a;\n  cursor: pointer;\n\n  .dropdown-title {\n    margin-right: 5px;\n    font-weight: 100;\n    font-size: 11px;\n  }\n\n  .fa-chevron-down {\n    font-size: 12px !important;\n  }\n}\n\n.no-operation {\n  display: block;\n  color: #f27a1a !important;\n  font-size: 11px;\n  font-weight: normal;\n}\n\n.btn-mp-outline,\n.btn-mp-primary {\n  white-space: nowrap;\n  line-height: 1.2;\n  margin-bottom: 15px;\n  width: 100%;\n  &:last-child {\n    margi