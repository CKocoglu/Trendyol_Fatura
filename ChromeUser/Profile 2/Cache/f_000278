');\n          text = this.$t('cancels.status.fraud');\n          break;\n      }\n\n      return {\n        icon,\n        text,\n        info,\n      };\n    },\n    cancellationReasonName () {\n      return this.line.packageItems[0].cancellationReasonName || this.line.packageItems[0].cancellationReason.name;\n    },\n    cancellationReason () {\n      return this.line.packageItems[0].cancellationReason;\n    },\n    isShowCustomerNote () {\n      return this.cancellationReasonName === 'CustomerRequest' && this.line.packageItems[0].cancellationReason === 'Dier';\n    },\n    isInstantUser () {\n      return window.user.storeBaseEnabled;\n    },\n    isShowCustomerNoteCancelConfig () {\n      return remoteConfig.getConfigValueByKey('IS_SHOW_CUSTOMER_NOTE_CANCEL');\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.cancel-reason {\n  position: relative;\n  white-space: nowrap;\n  .cancel-icon {\n    margin-right: 10px;\n  }\n  &:hover {\n    .order-description {\n      opacity: 1;\n    }\n  }\n  .order-description {\n    white-space: pre-line;\n    background-color: #afbbca;\n    color: #fff;\n    border-radius: 6px;\n    padding: 5px 10px;\n    position: absolute;\n    left: 0;\n    margin: 0 auto;\n    bottom: -6px;\n    transform: translate(0, -50%);\n    opacity: 0;\n    transition: opacity .3s ease-out;\n\n    &:before {\n      content:'';\n      display:block;\n      width:0;\n      height:0;\n      position:absolute;\n      border-right: 8px solid transparent;\n      border-left: 8px solid transparent;\n      border-top: 8px solid #afbbca;\n      top: 100%;\n      left: 10px;\n    }\n  }\n\n  .cancellation-reason {\n    display: block;\n    margin-top: 5px;\n  }\n\n  .customer-note {\n    .header {\n      font-size: 11px;\n      color: #273142;\n    }\n    .text {\n      font-size: 10px;\n      font-weight: normal;\n      font-style: italic;\n      color: #999999;\n    }\n  }\n\n  &::v-deep .app-tooltip {\n    white-space: normal;\n  }\n}\n</style