s.status === 'Rejected' || this.status === 'Unresolved' || this.status === 'InAnalysis';\n    },\n    isBarcodeLinkVisible () {\n      return this.status === 'Rejected';\n    },\n    isTakeActionVisible () {\n      const manualShipmentDeliveryAllowed =\n          this.$store.getters.manualShipmentDeliveryAllowed;\n\n      return this.status === 'Created' && manualShipmentDeliveryAllowed;\n    },\n    autoApproveDate () {\n      return this.claim.autoApproveDate;\n    },\n    setRemainingTime () {\n      const remainingTime = (this.autoApproveDate / 1000) - this.now;\n      const seconds = format.twoDigits(Math.trunc(remainingTime % 60));\n      const minutes = format.twoDigits(Math.trunc((remainingTime) / 60) % 60);\n      const hours = format.twoDigits(Math.trunc((remainingTime) / 60 / 60) % 24);\n      const days = Math.trunc((remainingTime) / 60 / 60 / 24);\n\n      if ((this.autoApproveDate / 1000) < this.now) {\n        return {\n          days: '0',\n          hours: '00',\n          minutes: '00',\n          seconds: '00',\n        };\n      }\n\n      return {\n        days,\n        hours,\n        minutes,\n        seconds,\n      };\n    },\n    hourglassImg () {\n      return images.setImagePublicPath('hourglass.svg');\n    },\n    isTypeRejectedOutbound () {\n      const routeMetaType = this.$route.meta.statuses;\n      return routeMetaType === 'Rejected';\n    },\n    isUpsCargoProvider () {\n      const shipmentPackageFilterDto = this.claim.shipmentPackageFilterDto;\n      return shipmentPackageFilterDto.cargoProviderName === CargoProviders.all.UPS_MP.name;\n    },\n    isConfirmActionButtonsVisible () {\n      return this.status === 'InAnalysis' || this.status === 'Unresolved';\n    },\n  },\n  asyncComputed: {\n    async isDeciBoxQuantityVisible () {\n      const shipmentPackageFilterDto = this.claim.shipmentPackageFilterDto;\n      if (shipmentPackageFilterDto) {\n        if (shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.UPS_MP.id) {\n          if (shipmentPackageFilterDto.boxQuantity === null &&\n            (shipmentPackageFilterDto.whoPays === null || shipmentPackageFilterDto.whoPays === 0)) {\n            return true;\n          }\n        } else if ((shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.CEVA_MP.id ||\n          shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.HOROZ_MP.id ||\n          shipmentPackageFilterDto.cargoProviderId === CargoProviders.all.BOR_MP.id)) {\n          if ((shipmentPackageFilterDto.boxQuantity === null && shipmentPackageFilterDto.deci === null) &&\n            (shipmentPackageFilterDto.whoPays === null || shipmentPackageFilterDto.whoPays === 0)) {\n            return true;\n          }\n        }\n      }\n      return false;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .waiting-timer {\n    margin-bottom: 10px;\n    display: flex;\n    .timer-text {\n      margin-left: 5px;\n    }\n    p {\n      font-size: 11px;\n      font-weight: normal;\n      color: #4a4a4a;\n      margin-bottom: 0;\n    }\n\n    span {\n      font-size: 12px;\n      color: #d0021b;\n    }\n  }\n\n  .actions-waiting {\n   min-width: 150px;\n\n   .green {\n     margin-right: 10px;\n   }\n  }\n\n  .print-label-info-ok {\n    margin-top: 10px;\n  }\n\n  .actions-waiting, .actions-new, .actions-in-analysis {\n   .btn {\n     padding: 5px;\n     font-size: 12px;\n     font-weight: 500;\n   }\n  }\n\n  .actions-in-analysis {\n    .btn {\n      width: 100%;\n      margin-bottom: 10px;\n    }\n  }\n\n  .outbound-shipment {\n    cursor: pointer;\n    margin-top: 10px;\n    font-size: 12px;\n    color: #f27a1a;\n    font-weight: 400;\n  }\n\n  .see-detail {\n    display: block;\n    cursor: pointer;\n    font-size: 12px;\n    color: #afbbca;\n    font-weight: normal;\n    i {\n      font-size: 8px !important;\n      cursor: pointer;\n      color: #afbbca;\n\n      &.fa-border {\n        padding: 0.4em 0.65em .3em;\n        border: solid 0.08em 