          duration: 7000,\n        });\n      } else {\n        const formData = new FormData();\n        if (files.length) {\n          _.each(files, f => formData.append('files', f));\n        }\n\n        Object.keys(options).forEach(key => {\n          formData.append(key, options[key]);\n        });\n\n        const payload = {\n          id: this.item.claimId,\n          list: this.orderLineIdList,\n          form: formData,\n        };\n\n        await this.rejectClaim(payload);\n        this.$toast.success('Talebiniz alnmtr. Bu ilemin takip edilmesi iin bildirim amanza gerek yoktur. htilafl sekmesinden takibini yapabilrsiniz.', {\n          duration: 5000,\n        });\n\n        if (!this.item.claimItemFilterGroupDtos.length) {\n          window.scEventBus.publisher('CHANGE_CLAIM_ITEM_COUNT', {\n            claimStatusToIncrement: 'unresolvedCount',\n            claimStatusToDecrement: 'waitingInActionCount',\n          });\n        }\n\n        this.close();\n        this.trackEvent();\n      }\n    },\n    trackEvent () {\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        category: 'PartnerPortal',\n        action: 'ClaimIssueCreate',\n        label: this.item.orderNumber,\n      });\n    },\n    countdown () {\n      this.remainingCount = this.maxCount + this.description.length;\n    },\n    dragDropFileSelectError (exception) {\n      this.$toast.error(exception.message, {\n        duration: 5000,\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .custom-file-btn{\n    display: block;\n    .custom-file-input{\n      display: none;\n    }\n  }\n  .custom-file {\n    width: 100%;\n\n    &-control:before {\n      content: \"\";\n    }\n  }\n\n  .remaining-count {\n    margin-top:2px;\n    text-align:right;\n    font-size: 13px;\n    color: Gray\n  }\n\n  .required-message {\n    color: red;\n    font-size: 11px;\n  }\n\n  .claim-issue::v-deep {\n    .modal-dialog {\n      max-width: 850px;\n    }\n\n    .modal-header