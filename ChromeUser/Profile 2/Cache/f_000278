= [];\n        filteredLines.forEach((q) => {\n          const line = q;\n          const newLine = Object.assign({}, line, {});\n\n          const groupedPackage = line.packageItems.reduce((a, val) => {\n            const acc = a;\n            (acc[val.cancellationReasonName || val.cancellationReason.name] = acc[val.cancellationReasonName || val.cancellationReason.name] || []).push(val);\n            return acc;\n          }, {});\n\n          if (Object.keys(groupedPackage).length > 1) {\n            delete groupedPackage[Object.keys(groupedPackage)[0]];\n            Object.keys(groupedPackage).forEach((key) => {\n              const packageItems = groupedPackage[key];\n              newLine.packageItems = packageItems;\n              newlines.push(newLine);\n              line.packageItems = line.packageItems\n                .filter(pItem => packageItems.indexOf(pItem) < 0);\n            });\n          }\n        });\n        filteredLines = filteredLines.concat(newlines);\n        item.lines = filteredLines;\n        return item;\n      });\n\n      if (this.cancels && this.cancels.pagination) {\n        this.tableConfig.pagination = {\n          page: this.cancels.pagination.page,\n          size: this.cancels.pagination.size,\n          totalElements: this.cancels.pagination.totalElements,\n          totalPages: this.cancels.pagination.totalPages,\n        };\n      }\n      this.isTableLoading = false;\n    },\n    handleFilter (data) {\n      const mergedQueryParams = { ...this.params, ...data };\n      this.params = mergedQueryParams;\n      this.params.page = 0;\n      this.handleTable();\n    },\n    page (p) {\n      this.params.page = p;\n      this.handleTable().then(() => {\n        window.scrollTo(0, 0);\n      });\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: window.user.storeBaseEnabled ? GAEvents.CATEGORY.CANCEL_INSTANT : GAEvents.CATEGORY.CANCEL_SC,\n        eventAction: `${this.$route.name} - ${GAEvents.ACTION.PAGINATION}`,\n        eventLabel: p + 1,\n      });\n    },\n    changePageSize (size) {\n      this.params.size = size;\n      this.handleTable().then(() => {\n        window.scrollTo(0, 0);\n      });\n    },\n    formatDate (value) {\n      return format.date(value);\n    },\n    formatPrice (value) {\n      return format.price(value);\n    },\n    dropdownOnSelect () {\n      this.params.storeId = this.branch && this.branch.id ? String(this.branch.id) : null;\n      this.handleTable();\n    },\n  },\n  computed: {\n    ...mapState([\n      'cancels',\n    ]),\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .list-header {\n    font-size: 16px;\n    font-weight: 500;\n    color: #273142;\n  }\n  .cancel-date {\n    position: relative;\n    white-space: nowrap;\n  }\n</style>\n","import mod from \"-!../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../node_modules/thread-loader/dist/cjs.js!../../../node_modules/babel-loader/lib/index.js!../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./List.vue?vue&type=script&lang=js&\"; export default mod; export * from \"-!../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../node_modules/thread-loader/dist/cjs.js!../../../node_modules/babel-loader/lib/index.js!../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./List.vue?vue&type=script&lang=js&\"","import { render, staticRenderFns } from \"./List.vue?vue&type=template&id=713a4351&scoped=true&\"\nimport script from \"./List.vue?vue&type=script&lang=js&\"\nexport * from \"./List.vue?vue&type=script&lang=js&\"\nimport style0 from \"./List.vue?vue&type=style&index=0&id=713a4351&lang=scss&scoped=true&\"\n\n\n/* normalize component */\nimport normalizer from \"!../../../node_modules/vue-loader/lib/runtime/componentNormalizer.js\"\nvar component = normalizer(\n  script,\n  render,\n  staticRenderFns,\n 