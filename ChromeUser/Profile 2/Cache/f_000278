};\n    },\n    isTimerFinished () {\n      return (this.agreedDeliveryDate / 1000) < this.now;\n    },\n    isTimerVisible () {\n      const status = this.status;\n      return status === 'Created' || status === 'Invoiced' || status === 'Picking' || status === 'ReadyToShip';\n    },\n    hourglassImg () {\n      return images.setImagePublicPath('hourglass.svg');\n    },\n    originShipmentDate () {\n      if (window.user.storeBaseEnabled) {\n        return format.date(this.order.orderDate);\n      } else {\n        return format.date(this.order.originShipmentDate);\n      }\n    },\n    packageDayShipping () {\n      var difference = new Date() - this.order.shipmentDate;\n      const days = Math.trunc((difference) / 1000 / 60 / 60 / 24);\n      if (days > 0) {\n        this.setShipmentDayColor(days);\n        return `${days} gndr kargoda`;\n      }\n      return 'Bugn kargoya verildi';\n    },\n    isInstantUser () {\n      return window.user.storeBaseEnabled;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.order-info {\n  position: relative;\n  ::v-deep .app-tooltip {\n    .icon-exclamation-circle {\n      font-size: 15px;\n\n      &:before {\n        color: #dc3545;\n      }\n    }\n\n    .tooltip-text {\n      span {\n        width: 150px;\n        white-space: normal;\n        padding: 13px;\n      }\n    }\n  }\n\n  .with-text {\n    position: relative;\n    white-space: nowrap;\n\n    &:hover {\n      .order-description {\n        opacity: 1;\n      }\n    }\n\n    .order-description {\n      white-space: nowrap;\n      background-color: #afbbca;\n      color: #fff;\n      text-align: center;\n      border-radius: 6px;\n      padding: 5px 10px;\n      position: absolute;\n      left: 0;\n      margin: 0 auto;\n      top: -25px;\n      transform: translate(0, -50%);\n      opacity: 0;\n      transition: opacity .3s ease-out;\n\n      &:before {\n        content: '';\n        display: block;\n        width: 0;\n        height: 0;\n        positio