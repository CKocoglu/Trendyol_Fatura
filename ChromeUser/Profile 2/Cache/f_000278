PAKET NO') === undefined) {\n        this.tableConfig.columns = [\n          ...this.tableConfig.columns.slice(0, 1),\n          { label: 'PAKET NO' },\n          ...this.tableConfig.columns.slice(1),\n        ];\n      }\n\n      this.tableConfig.body = allClaims.content;\n\n      this.tableConfig.pagination = {\n        page: this.claims.pagination.page,\n        size: this.claims.pagination.size,\n        totalElements: this.claims.pagination.totalElements,\n        totalPages: this.claims.pagination.totalPages,\n      };\n      this.isTableLoading = false;\n    },\n    handleFilter () {\n      this.handleTable();\n    },\n    page (p) {\n      this.claims.filterParams.page = p;\n      this.handleTable().then(() => {\n        window.scrollTo(0, 0);\n      });\n      this.$gtm.trackEvent({\n        event: 'GAEvent',\n        eventCategory: GAEvents.CATEGORY.CLAIM_SC,\n        eventAction: `${this.$route.name} - ${GAEvents.ACTION.PAGINATION}`,\n        eventLabel: p + 1,\n      });\n    },\n    formatPrice (value) {\n      return format.price(value);\n    },\n    accept () {\n      this.acceptModal = true;\n    },\n    issue () {\n      this.issueModal = true;\n    },\n    details () {\n      this.detailModal = true;\n    },\n    takeAction () {\n      this.isConfirmDialogOpen = true;\n    },\n    async acceptTakeAction () {\n      await this.setClaimStatusToWaitingInAction();\n      this.$toast.success('Talebiniz baaryla alnmtr.', {\n        duration: 5000,\n      });\n      window.scEventBus.publisher('CHANGE_CLAIM_ITEM_COUNT', {\n        claimStatusToIncrement: 'waitingInActionCount',\n        claimStatusToDecrement: 'createdCount',\n      });\n      this.isConfirmDialogOpen = false;\n    },\n    async barcode (type) {\n      const shipmentPackageFilterDto = this.claims.selectedItem.shipmentPackageFilterDto;\n      const claim = this.claims;\n\n      this.selectedOrders = [];\n\n      if (shipmentPackageFilterDto) {\n        if (shipmentPackageFilterDto.cargoPro