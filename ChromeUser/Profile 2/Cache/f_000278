    reject(error);\n      };\n\n      fileReader.readAsArrayBuffer(file);\n    });\n  }\n\n  static toJson (excel, options = {}) {\n    const sheetName = excel.SheetNames && excel.SheetNames[0];\n\n    return sheetJS.utils.sheet_to_json(excel.Sheets[sheetName], options);\n  }\n}\n","import Excel from '@/lib/Excel';\n\nclass CargoChangeExcelValidator {\n  constructor () {\n    this.validHeaders = ['Paket Numaras', 'Kargo irketi'];\n  }\n  validate ({ excel }) {\n    const excelAsJson = Excel.toJson(excel, { header: 1 });\n\n    return this.validateHeaders(excelAsJson);\n  }\n\n  validateHeaders (excelAsJson = []) {\n    const excelHeader = excelAsJson[0];\n\n    const isValid = this.validHeaders.every((headerTitle, index) => {\n      return excelHeader[index] === headerTitle;\n    });\n\n    if (isValid) {\n      return { valid: true };\n    }\n\n    return {\n      valid: false,\n      error: {\n        key: 'NOT_MATCH_HEADER',\n      },\n    };\n  }\n}\n\nexport default new CargoChangeExcelValidator();\n","class RemoteConfigService {\n  constructor (remoteConfig) {\n    this.config = remoteConfig;\n  }\n\n  /**\n   * Freeze given object and set to config\n   * @param config\n   */\n  setConfig (config) {\n    this.config = Object.freeze(config);\n  }\n\n  /**\n   * Returns config object\n   * @returns {{}}\n   */\n  getConfig () {\n    return this.config;\n  }\n\n  /**\n   * Returns the value of given key\n   * @param key\n   * @returns {*}\n   */\n  getConfigValueByKey (key) {\n    return this.getConfig()[key];\n  }\n}\n\nexport default new RemoteConfigService(window.remoteConfig);\n","<template>\n  <div class=\"bulk-order-operations\">\n    <div class=\"header\">\n      <div class=\"left\">\n        <span class=\"title\">\n          {{ $t('bulkOrderOperations.header.title') }}\n        </span>\n        <ScTooltip>\n          <template #tooltip-text>\n            {{ $t('bulkOrderOperations.header.tooltip') }}\n          </template>\n          <template #tooltip-tr